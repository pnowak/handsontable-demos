<!doctype html>
<html lang=en>
<head>
  <%- partial("_partials/_head") %>
</head>
<body>
  <section>

    <ul id="tabs">
      <li class="<%= current.source=='index'?'selected':'' %>">
        <a href="/index">Highcharts</a>
      </li>
      <li class="<%= current.source=='amCharts'?'selected':'' %>">
        <a href="/amCharts">amCharts</a>
      </li>
      <li class="<%= current.source=='chartJs'?'selected':'' %>">
        <a href="/chartJs">Chart.js</a>
      </li>
      <li class="<%= current.source=='fusionCharts'?'selected':'' %>">
        <a href="/fusionCharts">FusionCharts</a>
      </li>
    </ul>

    <div id="root"></div>
    <div id="controls">
      <button type="button" value="addNewGame">Add new game</button>
      <button type="button" value="addNewTeam">Add new team</button>
    </div> 

    <%- yield %>

  </section>

  <script src="./assets/js/codeMirror.config.js" type="text/javascript"></script>
  <script src="./assets/js/jsFiddle.js" type="text/javascript"></script>
  <script src="./assets/js/bundle.js" type="text/javascript"></script>

</body>
</html>
